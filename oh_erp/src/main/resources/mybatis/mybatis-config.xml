<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<!-- mybatis를 운용하기 위한 설정이 있다면 이곳에 작성 -->
	
	<!-- properties : 이 문서 또는 외부에서 사용할 변수 -->
	<properties></properties>
	
	<!-- settings : myBatis 실행 중 반영되어야 할 규칙을 지정 -->
	<settings>
		<setting name="cacheEnabled" value="true"/>
		<setting name="lazyLoadingEnabled" value="true"/>
		<setting name="autoMappingBehavior" value="FULL"/>
	</settings>
	
	<!-- typeAliases : 클래스에 부여하는 별칭 -->
	<typeAliases>	
		<!-- <typeAlias type="com.kh.spring07.entity.StudentDto" alias="student"/> -->
		 <typeAlias type="com.kh.oherp.entity.DepartmentDto" alias="departmentDto"/>
		<typeAlias type="com.kh.oherp.entity.Attendance_detailDto" alias="attendance_detailDto"/> 
		<typeAlias type="com.kh.oherp.entity.Attendance_detailListVo" alias="attendance_detailListVo"/>      
		<typeAlias type="com.kh.oherp.entity.MemberDto" alias="memberDto"/>
		<typeAlias type="com.kh.oherp.entity.MemberListVo" alias="memberListVo"/>
		<typeAlias type="com.kh.oherp.entity.AttendanceRequestDto" alias="attendanceRequestDto"/>
		<typeAlias type="com.kh.oherp.entity.MemberRequestDto" alias="memberRequestDto"/>
		<typeAlias type="com.kh.oherp.entity.BoardDto" alias="boardDto"/>
		<typeAlias type="com.kh.oherp.entity.PhotoDto" alias="photoDto"/>		
	</typeAliases>
	
</configuration>



